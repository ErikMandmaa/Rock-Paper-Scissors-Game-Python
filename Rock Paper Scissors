import random

player = int(input("Pick a number from 1 to 3: "))

computer = random.randint(1, 3)

choices = {1: "rock", 2: "paper", 3: "scissors"}
print()

print(f"You chose: {choices[player]}")
print(f"The computer chose: {choices[computer]}")

if player == computer:
    print("It's a tie!")


